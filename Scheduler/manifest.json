{
  "manifest_version": 2,
  "name": "14_05",
  "version": "1.0",
  
  "browser_action": {
    "default_title": "15_05",
    "default_popup": "popup.html"  
  },

  "background": {
      "scripts": [

      "jquery.js", 
      "options_actions.js", 
      "log.js", 
      "hash.js", 
      "tabs.js", 
      "apps.js", 
      "time.js", 
      "storage.js", 
      "schedule.js",  
      "scheduler.js"

      ]
  },

  "options_page": "options.html",
  
  "permissions": [
  	"tabs", 
    "http://*/*", 
    "https://*/*", 
    "background", 
    "storage", 
    "notifications"

	],

  "web_accessible_resources": [
    "appScript.js"
  ],
  
  "content_security_policy": "script-src 'self' https://code.jquery.com/jquery.js; object-src 'self'"
}