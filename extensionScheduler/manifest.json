{
  "manifest_version": 2,
  "name": "07_04",
  "version": "1.0",
  
  "browser_action": {
    "default_title": "07_04",
    "default_popup": "popup.html"  
  },

  "background": {
    "scripts": ["scheduler.js","support.js","timer.js","jquery.js"]
  },

